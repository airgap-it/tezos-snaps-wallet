{
  "name": "tezos-snaps-wallet",
  "version": "0.0.0",
  "scripts": {
    "actions:deploy": "ng deploy --name $GITHUB_ACTOR --email $GITHUB_ACTOR@users.noreply.github.com --no-silent",
    "actions:prettier": "prettier \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.scss\" \"src/**/*.html\" \"e2e/**/*.ts\" \"angular.json\" \"README.md\" --list-different",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prettier": "prettier \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.scss\" \"src/**/*.html\" \"e2e/**/*.ts\" \"angular.json\" \"README.md\" --write",
    "fix-crypto": "node config/patch_crypto.js && npm run prepare-prod-build",
    "prepare-prod-build": "npm run disable-pure-getters && npm run configure-mangle",
    "disable-pure-getters": "replace 'pure_getters: buildOptions.buildOptimizer' 'pure_getters: false' ./node_modules/@angular-devkit/build-angular/src/webpack/configs/common.js ",
    "configure-mangle": "replace 'safari10: true' 'safari10: true, keep_fnames: true' ./node_modules/@angular-devkit/build-angular/src/webpack/configs/common.js "
  },
  "private": true,
  "dependencies": {
    "@airgap/beacon-core": "4.0.4",
    "@airgap/beacon-wallet": "4.0.4",
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@metamask/providers": "^11.1.0",
    "@taquito/rpc": "^10.0.0",
    "angularx-qrcode": "^12.0.3",
    "assert-polyfill": "*",
    "bignumber.js": "^9.1.1",
    "bootstrap": "^4.5.3",
    "bs58check": "^2.1.2",
    "buffer": "^5.7.1",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "json-formatter-js": "^2.3.4",
    "ngx-bootstrap": "^7.1.0",
    "ngx-timeago": "^2.0.0",
    "ngx-toastr": "^14.3.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.6.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tslib": "^2.2.0",
    "util": "^0.12.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular/cli": "~12.1.4",
    "@angular/compiler-cli": "~12.1.0",
    "@types/bs58check": "^2.1.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "*",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^3.0.3",
    "replace": "^1.2.1",
    "typescript": "~4.3.2"
  }
}
